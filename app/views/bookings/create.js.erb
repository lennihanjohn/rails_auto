<% if @errors.nil? %>
  swal({
    title: "success",
    text: "saved",
    type: "success",
    showConfirmButton: true,
    allowEscapeKey: false
  }).then((result) => {
    if (result) {
        $("#type_of_repair_modal").modal("hide")
        $("#search_result").html("<%= j render partial: 'type_of_repairs/partials/table'%>")
    }
  });

<% else %>
console.log("hello")
swal({
    title: "Oops",
    text: "<%= @errors.html_safe %>",
    type: "error"
})


<% end %>
