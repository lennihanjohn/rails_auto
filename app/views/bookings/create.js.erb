<% if @errors.blank? %>
  swal({
    title: "success",
    text: "saved",
    type: "success",
    showConfirmButton: true,
    allowEscapeKey: false
  }).then((result) => {
    if (result) {
        $("#booking_modal").modal("hide")
        $("#search_result").html("<%= j render partial: 'bookings/partials/table'%>")
    }
  });

<% else %>
console.log("hello")
swal({
    title: "Oops",
    text: '<%=j raw @errors%>',
    type: "error"
})


<% end %>
