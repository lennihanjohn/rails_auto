<table class="table">
    <thead class="thead-dark">
    <tr>
        <th scope="col">Name</th>
        <th scope="col">Phone # </th>
        <th scope="col">Date </th>
        <th scope="col">Start Time</th>
        <th scope="col">End Time</th>
        <th scope="col"></th>

        </tr>
    </thead>
    <tbody>
        <% @bookings.each do |booking| %>
            <tr>
                <td><%= booking.id.to_s + "," + booking.user.name %></td>
                <td><%= booking.user.phone_number %></td>
                <td><%= booking.repair_date.strftime('%b/%d/%Y') %></td>
                <td><%= booking.start_at.strftime("%H:%M %p") %></td>
                <td><%= booking.end_at.strftime("%H:%M %p") %></td>
                <td><%= booking.is_active ? '<i class="far fa-check-circle text-success"></i>'.html_safe : '<i class="far fa-times-circle text-danger"></i>'.html_safe  %></td>
                <th>
                    <%#= link_to('<i class="fa fa-pen" aria-hidden="true"></i>'.html_safe, edit_day_of_business_hour_path(id:day_of_week.day_of_business_hour.id), class:"btn btn-info btn-sm", remote: true) %>
                </th>
            </tr> 
        <% end %>
    </tbody>
</table>

